# Environment Variables Configuration Template
# Copy this file to .env and fill in your actual values

# Database Configuration
# Replace YOUR_PASSWORD with your PostgreSQL password
DATABASE_URL=postgresql://postgres:YOUR_PASSWORD@localhost:5432/finalserver_db

# JWT Configuration
# Generate a new secret key using: python -c "import secrets; print(secrets.token_hex(32))"
SECRET_KEY=your-secret-key-here-generate-a-new-one
ALGORITHM=HS256
ACCESS_TOKEN_EXPIRE_MINUTES=30

# Server Configuration
HOST=0.0.0.0
PORT=8000
